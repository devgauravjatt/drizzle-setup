{
	"name": "drizzle-setup",
	"version": "0.0.1",
	"private": true,
	"description": "A simple example library built by tsdown",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"bin": "./dist/index.js",
	"scripts": {
		"start": "node ./dist/index.js",
		"build": "tsdown",
		"dev": "bun src/index.ts",
		"copy-templates": "xcopy \"templates\" \"dist\\templates\" /E /I /Y",
		"link": "bun run build && bun copy-templates && bun link"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.15.30",
		"drizzle-kit": "^0.31.1",
		"drizzle-orm": "^0.44.2",
		"tsdown": "latest",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"execa": "^9.6.0",
		"fs-extra": "^11.3.0",
		"path": "^0.12.7",
		"url": "^0.11.4"
	}
}
